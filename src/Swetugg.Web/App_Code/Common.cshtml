@using Microsoft.AspNetCore.Mvc.Razor
@{ HelperResult GetPublished(bool published)
	{
	    if (published)
	    {
	        <i class="glyphicon glyphicon-check" title="Published"></i>
	    }
	    else
	    {
	        <i class="glyphicon glyphicon-unchecked" title="NOT Published"></i>
	    }
		return new HelperResult(w => Task.CompletedTask);
} }
