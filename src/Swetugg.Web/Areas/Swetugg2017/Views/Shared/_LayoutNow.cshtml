<body id="page-top" data-spy="scroll" data-target=".navbar-custom">
    <div id="fb-root"></div>
    <script>
        (function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) return;
            js = d.createElement(s);
            js.id = id;
            js.src = "//connect.facebook.net/sv_SE/sdk.js#xfbml=1&version=v2.3&appId=@fbAppId";
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));
    </script>

    <nav class="navbar navbar-custom navbar-fixed-top @(transformNavBar ? "transform": "top-nav-collapse" )" role="navigation">
        <div class="container">
            <div class="navbar-header">
                <a href="." class="navbar-toggle">
                    <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
                </a>
                <a class="navbar-brand" href="@Url.Action("Index", "Conference")#page-top" data-target="#page-top">
                    <img src="/Content/swetugg-2017/img/40_WhiteBG_GraySwetuggOrigamiButterfly.png" width="50" height="41" class="pull-left"/>
                    Swetugg
                </a>
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse navbar-right navbar-main-collapse">
            </div>
            <!-- /.navbar-collapse -->
        </div>
        <!-- /.container -->
    </nav>

